
public class Main
{
	public static void main(String args[]) {
	int n=6;
	boolean b=isPerfect(n);
	if(b) {
		System.out.println("perfect");
	}
	else {
		System.out.println("NOT perfect");
	}
	}
	public static boolean isPerfect(int n) {
		int sum=0;
		for(int i=1;i<n;i++) {
			if(n%i==0) {
				sum+=i;
			}
		}
		if(n==sum) {
			return true;
		}
		else
			return false;
	}
}


//n=6:
// when you get n=sum of its factors then its a perfect number

    


